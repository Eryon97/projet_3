{% extends "layout.html.twig" %}

{% block title %}Home{% endblock %}

{% block content %}
{% for article in articles %}
<article>
    <div class="well" style='padding-bottom: 10px;'>
        <h2><a class="articleTitle" href="{{ path('article', { 'id': article.id }) }}">{{ article.title }}</a></h2>
        <p>{{ article.content }}</p>
    </div>
</article>
{% endfor %}
{% endblock %}
