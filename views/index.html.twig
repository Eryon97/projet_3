{% extends "layout.html.twig" %}

{% block title %}Home{% endblock %}

{% block header %}
{% autoescape 'html' %}
<div class="header">
    <a href="http://www.hlgjyl888.com/WDF-2511129.html"><img src="https://img4.hostingpics.net/pics/124375WDF2511129.jpg" width="100%" height="400px"/></a>
</div>
{% endautoescape %}
{% endblock %}

{% block content %}



{% for article in articles %}
<article>
    <div class="well">
        <h2><a class="articleTitle" href="{{ path('article', { 'id': article.id }) }}">{{ article.title }}</a></h2>
        <p>{{ article.content }}</p>
    </div>
</article>
{% endfor %}
{% endblock %}
